{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    layout ? 'layout--' ~ layout|clean_class,
  ]
%}


<div{{attributes.addClass(classes)}}>
	{{ attach_library('lelohex/content-css') }}
	{% block content %}
		{{ content.field_video }}
	{% endblock %}
</div>
